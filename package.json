{
  "name": "gulp-cli",
  "version": "2.3.0",
  "description": "Command line interface for gulp",
  "author": "Gulp Team <team@gulpjs.com> (https://gulpjs.com/)",
  "contributors": [],
  "homepage": "https://gulpjs.com",
  "repository": "gulpjs/gulp-cli",
  "license": "MIT",
  "man": "gulp.1",
  "engines": {
    "node": ">=10.13.0"
  },
  "main": "index.js",
  "bin": {
    "gulp": "bin/gulp.js"
  },
  "files": [
    "index.js",
    "lib",
    "bin",
    "completion",
    "gulp.1"
  ],
  "scripts": {
    "lint": "eslint .",
    "prepare": "marked-man --name gulp docs/CLI.md > gulp.1",
    "pretest": "npm run lint",
    "test": "nyc mocha --async-only --timeout 5000 test/lib test"
  },
  "dependencies": {
    "archy": "^1.0.0",
    "chalk": "^4.1.2",
    "concat-stream": "^2.0.0",
    "copy-props": "^4.0.0",
    "fancy-log": "^2.0.0",
    "gulplog": "^2.0.1",
    "interpret": "^1.4.0",
    "liftoff": "^3.1.0",
    "matchdep": "^2.0.0",
    "mute-stdout": "^2.0.0",
    "pretty-hrtime": "^1.0.3",
    "replace-homedir": "^2.0.0",
    "semver-greatest-satisfied-range": "^2.0.0",
    "v8flags": "^3.2.0",
    "yargs": "^7.1.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.26.0",
    "eslint": "^7.32.0",
    "eslint-config-gulp": "^5.0.1",
    "expect": "^1.20.2",
    "gulp": "^4.0.2",
    "marked-man": "^0.2.1",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.8"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "keywords": [
    "build",
    "stream",
    "system",
    "make",
    "tool",
    "asset",
    "pipeline"
  ]
